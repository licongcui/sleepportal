<h1>Listing report_concepts</h1>

<table>
  <tr>
    <th>Report</th>
    <th>Concept</th>
    <th>Position</th>
    <th>Strata</th>
    <th>Statistic</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @report_concepts.each do |report_concept| %>
  <tr>
    <td><%= report_concept.report_id %></td>
    <td><%= report_concept.concept_id %></td>
    <td><%= report_concept.position %></td>
    <td><%= report_concept.strata %></td>
    <td><%= report_concept.statistic %></td>
    <td><%= link_to 'Show', report_concept %></td>
    <td><%= link_to 'Edit', edit_report_concept_path(report_concept) %></td>
    <td><%= link_to 'Destroy', report_concept, data: { confirm: 'Are you sure?' }, method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

