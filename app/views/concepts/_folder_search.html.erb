<%= form_tag search_folder_concepts_path(query_id: @query.id, add_report_id: params[:add_report_id]), remote: true, method: :post, onsubmit: "showWaiting('#concept_folders', ' Loading Concepts', true)", id: "concept_search_form" do %>
  <%= text_field_tag 'concept_search', params[:concept_search], style: 'width:355px' %>
<% end %>
<fieldset>
  <legend>Key</legend>

  <div class="field">
    <%= image_tag("icons/closed.png", style: "vertical-align:text-bottom") %> Navigate folders by clicking the expand folder icon.
  </div>
  <br />
  <div class="field">
    <%= image_tag("contour/information.png", style: "vertical-align:text-bottom") %> Click on a concept for a detailed description.
  </div>
  <br />
  <div class="field common">
    <%= image_tag("contour/blank.png", style: "vertical-align:text-bottom") %> <%= link_to 'Commonly Used Concept', '#' %>
  </div>
  <br />
  <div class="field uncommon">
    <%= image_tag("contour/blank.png", style: "vertical-align:text-bottom") %> <%= link_to 'Uncommon Concept', '#' %>
  </div>
</fieldset>
<%#= text_field_tag 'concept_search', '', onchange: remote_function(url: search_folder_concepts_path(query_id: @query.id), method: :post, with: "$('concept_search').serialize()", before: "showWaitingNew('concept_folders', true);") + "; return false;" %>