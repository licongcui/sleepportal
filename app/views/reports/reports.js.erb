<% original_report = @report %>
<% if @report.is_dataset? %>
  $("#datasets_container").html("<%= escape_javascript(render("reports/datasets")) %>");
  $("#datasets_list").html("<%= escape_javascript(render("reports/datasets_list")) %>");
<% else %>
  $("#reports_container").html("<%= escape_javascript(render("reports/reports")) %>");
  $("#reports_list").html("<%= escape_javascript(render("reports/reports_list")) %>");
<% end %>
<% @report = original_report %>

<% params[:add_report_id] = @report.id %>

<%# Only render if report exists (it's not been deleted) %>
<% if @report.destroyed? %>
  $("#inlaid_report").html("");
<% else %>
  <%= render partial: 'reports/load' %>
<% end %>
