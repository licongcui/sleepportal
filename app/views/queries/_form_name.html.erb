<%= text_field :query, :name, class: "input-large", placeholder: "Enter a Name for your Search", style: 'margin-bottom:0px', data: { query_id: @query.id } %>
<%= link_to 'Save Name', '#', class: 'btn btn-primary btn-mini', data: { object: 'save_query_name' } %>
<%= link_to 'Cancel', @query, method: :get, remote: true, class: 'btn btn-mini' %>
